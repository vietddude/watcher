          "format": "table",
          "instant": true,
          "legendFormat": "{{chain}} - {{provider}}",
          "refId": "B"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "label_join(sum by (chain, provider) (rate(watcher_rpc_calls_total{chain=~\"$chain\"}[5m])), \"key\", \" - \", \"chain\", \"provider\")",
          "format": "table",
          "instant": true,
          "legendFormat": "{{chain}} - {{provider}}",
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "label_join((sum by (chain, provider) (rate(watcher_rpc_errors_total{chain=~\"$chain\"}[5m])) / sum by (chain, provider) (rate(watcher_rpc_calls_total{chain=~\"$chain\"}[5m]))) > 0.05 or (sum by (chain, provider) (rate(watcher_rpc_latency_seconds_sum{chain=~\"$chain\"}[5m])) / sum by (chain, provider) (rate(watcher_rpc_latency_seconds_count{chain=~\"$chain\"}[5m]))) > 1, \"key\", \" - \", \"chain\", \"provider\")",
          "format": "table",
          "instant": true,
          "legendFormat": "{{chain}} - {{provider}}",
          "refId": "D"
        }
      ],
      "title": "Provider Health",
      "transformations": [
        {
          "id": "joinByField",
          "options": {
            "byField": "key",
            "mode": "outer"
          }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "Time 3": true,
              "Value #D": true,
              "chain": true,
              "provider": true
            },
            "indexByName": {},
            "renameByName": {
              "Value #A": "Avg Latency (ms)",
              "Value #B": "Error %",
              "Value #C": "RPS",
              "key": "Provider"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "Error %",
              "operator": ">",
              "right": "5"
            },
            "alias": "is_error_high"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "Avg Latency (ms)",
              "operator": ">",
              "right": "1000"
            },
            "alias": "is_latency_high"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            },
            "binary": {
              "left": "is_error_high",
              "operator": "+",
              "right": "is_latency_high"
            },
            "alias": "Status"
          }
        }
      ],
      "type": "table"
    },
